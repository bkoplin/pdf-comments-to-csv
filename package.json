    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.23.0",
    "@antfu/ni": "^0.22.0",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^20.14.11",
    "@vitest/ui": "^2.0.5",
    "bumpp": "^9.4.1",
    "eslint": "^9.7.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.7",
    "pnpm": "^9.5.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0",
    "vite": "^5.3.4",
    "vitest": "^2.0.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
