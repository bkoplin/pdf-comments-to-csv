    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.30",
    "colord": "^2.9.3",
    "lodash-es": "^4.17.21",
    "magic-regexp": "^0.8.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "papaparse": "^5.4.1",
    "turndown": "^7.2.0",
